# Those paths are just a reference. modify them to match your setup.
cd /root/SGDK-master
export GDK=$(pwd) 
export GDK_WIN=$(pwd) 
export SGDK_SRC=$(pwd) 
export PATH=$PATH:/root/tchainAndroidM68kSGDK/m68k-elf/bin
export PATH=$PATH:$SGDK_SRC/tools/apj/src/sgdk/aplib:$SGDK_SRC/tools/bintos/src/build
export PATH=$PATH:$SGDK_SRC/tools/commons/src/sgdk/tool:$SGDK_SRC/tools/sjasm/src

if [ -z "${C_INCLUDE_PATH+x}" ]; then
    export C_INCLUDE_PATH=/$GDK_WIN/res/:/$GDK_WIN/inc/
else
    export C_INCLUDE_PATH=$C_INCLUDE_PATH:/$GDK_WIN/res/:/$GDK_WIN/inc/
fi